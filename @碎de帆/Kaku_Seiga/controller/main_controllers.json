{
	"format_version": "1.19.0",
	"animation_controllers": {
		"player.main": {
			"initial_state": "transfer",
			"states": {
				"transfer": {
					"animations": [
						{"idle": "ctrl.idle"},
						{"walk": "ctrl.walk"},
						{"run": "ctrl.run"},
						{"jump": "ctrl.jump"},
						{"sneak": "ctrl.sneak"},
						{"sneaking": "ctrl.sneaking"},
						{"swim": "ctrl.swim"},
						{"swim_stand": "ctrl.swim_stand"},
						{"climb": "ctrl.climb"},
						{"climbing": "ctrl.climbing"},
						{"attacked": "ctrl.attacked"},
						{"death": "ctrl.death"},
						{"riptide": "ctrl.riptide"},
						{"ladder_up": "ctrl.ladder_up"},
						{"ladder_stillness": "ctrl.ladder_stillness"},
						{"ladder_down": "ctrl.ladder_down"},
						{"fly": "ctrl.fly"},
						{"elytra_fly": "ctrl.elytra_fly"}
					],
					"transitions": [
						{"idle": "ctrl.idle"},
						{"walk": "ctrl.walk"},
						{"run": "ctrl.run"},
						{"jump": "ctrl.jump || query.is_riding"},
						{"sneaking": "ctrl.sneaking"},
						{"sneak": "ctrl.sneak"},
						{"swim": "ctrl.swim"},
						{"swim_stand": "ctrl.swim_stand"},
						{"riptide": "ctrl.riptide"},
						{"climbing": "ctrl.climbing"},
						{"climb": "ctrl.climb"},
						{"ladder_up": "ctrl.ladder_up"},
						{"ladder_stillness": "ctrl.ladder_stillness"},
						{"ladder_down": "ctrl.ladder_down"},
						{"attacked": "ctrl.attacked"},
						{"death": "ctrl.death"},
						{"fly": "ctrl.fly"},
						{"elytra_fly": "ctrl.elytra_fly"},
						{"tlm_sit": "tlm.is_sitting"},
						{"sleep": "ctrl.sleep"}
					],
					"blend_transition": 0.15
				},
				"idle": {
					"animations": [
						{"idle": "ctrl.idle && !ctrl.tac_hold_gun && !ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white')"},
						{"slashblade:idle": "ctrl.idle && (ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white'))"},
						{"tac:idle": "ctrl.idle && ctrl.tac_hold_gun"}
					],
					"on_entry": ["v.idle_time1 = query.anim_time;"],
					"transitions": [
						{"idle_longtime": "query.anim_time - v.idle_time1 >= 15 && (1 - v.roaming.longtime_idle) == 1"},
						{"transfer": "!ctrl.idle || tlm.is_sitting"}
					],
					"blend_transition": 0.15
				},
				"idle_longtime": {
					"animations": ["idle_longtime"],
					"transitions": [
						{"idle": "query.all_animations_finished"},
						{"transfer": "!ctrl.idle || tlm.is_sitting"}
					],
					"blend_transition": 0.15
				},
				"tlm_sit": {
					"animations": ["sit"],
					"transitions": [
						{"transfer": "!tlm.is_sitting"}
					],
					"blend_transition": 0.15
				},
				"walk": {
					"animations": [
						{"walk": "!ctrl.tac_hold_gun  && !ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"tac:walk": "ctrl.tac_hold_gun && ctrl.parcool_state == ''"},
						{"slashblade:walk": "(ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.walk || tlm.is_sitting"}
					],
					"blend_transition": 0.15,
					"blend_via_shortest_path": true
				},
				"run": {
					"animations": [
						{"run": "!ctrl.tac_hold_gun  && !ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"tac:run": "ctrl.tac_hold_gun && ctrl.parcool_state == ''"},
						{"slashblade:run": "(ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.run"}
					],
					"blend_transition": 0.15
				},
				"jump": {
					"animations": [
						{"jump": "!query.is_riding && !ctrl.sleep && !ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"slashblade:jump": "!query.is_riding && !ctrl.sleep && (ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.jump"}
					],
					"blend_transition": 0.15
				},
				"fly": {
					"animations": [
						{"fly": "!ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"slashblade:fly": "(ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.fly"}
					],
					"blend_transition": 0.15
				},
				"elytra_fly": {
					"animations": ["elytra_fly"],
					"transitions": [
						{"transfer": "!ctrl.elytra_fly"}
					],
					"blend_transition": 0.15
				},
				"sneaking": {
					"animations": [
						{"sneaking": "!ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"slashblade:sneaking": "(ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.sneaking"}
					],
					"blend_transition": 0.15
				},
				"sneak": {
					"animations": [
						{"sneak": "!ctrl.hold('mainhand', '$slashblade:slashblade') && !ctrl.hold('mainhand', '$slashblade:slashblade_wood') && !ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') && !ctrl.hold('mainhand', '$slashblade:slashblade_white') && ctrl.parcool_state == ''"},
						{"slashblade:sneak": "(ctrl.hold('mainhand', '$slashblade:slashblade') || ctrl.hold('mainhand', '$slashblade:slashblade_wood') || ctrl.hold('mainhand', '$slashblade:slashblade_bamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_silverbamboo') || ctrl.hold('mainhand', '$slashblade:slashblade_white')) && ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.sneak"}
					],
					"blend_transition": 0.15
				},
				"swim": {
					"animations": [
						{"swim": "ctrl.parcool_state == ''"},
						{"parcool:fast_swim": "ctrl.parcool_state == 'fast_swim'"}
					],
					"transitions": [
						{"transfer": "!ctrl.swim"}
					],
					"blend_transition": 0.15
				},
				"swim_stand": {
					"animations": ["swim_stand"],
					"transitions": [
						{"transfer": "!ctrl.swim_stand"}
					],
					"blend_transition": 0.15
				},
				"riptide": {
					"animations": ["riptide"],
					"transitions": [
						{"transfer": "!ctrl.riptide"}
					],
					"blend_transition": 0.15
				},
				"climbing": {
					"animations": ["climbing"],
					"transitions": [
						{"transfer": "!ctrl.climbing"}
					],
					"blend_transition": 0.15
				},
				"climb": {
					"animations": [
						{"climb": "ctrl.parcool_state == ''"}
					],
					"transitions": [
						{"transfer": "!ctrl.climb"}
					],
					"blend_transition": 0.15
				},
				"ladder_up": {
					"animations": ["ladder_up"],
					"transitions": [
						{"transfer": "!ctrl.ladder_up"}
					],
					"blend_transition": 0.15
				},
				"ladder_stillness": {
					"animations": ["ladder_stillness"],
					"transitions": [
						{"transfer": "!ctrl.ladder_stillness"}
					],
					"blend_transition": 0.15
				},
				"ladder_down": {
					"animations": ["ladder_down"],
					"transitions": [
						{"transfer": "!ctrl.ladder_down"}
					],
					"blend_transition": 0.15
				},
				"attacked": {
					"animations": ["attacked"],
					"transitions": [
						{"transfer": "!ctrl.attacked"}
					],
					"blend_transition": 0.15
				},
				"death": {
					"animations": ["death"],
					"transitions": [
						{"transfer": "!ctrl.death"}
					],
					"blend_transition": 0.15
				},
				"sleep": {
					"animations": ["sleep"],
					"transitions": [
						{"transfer": "!ctrl.sleep"}
					],
					"blend_transition": 0.15
				}
			}
		}
	}
}